{
	"require": {
		"laravel/framework": "5.4.*",
		"league/commonmark": "*",
		"laracasts/presenter": "*",
		"laravelcollective/html": "5.4.*",
		"dingo/api": "1.0.*@dev",
		"pespantelis/gravatar": "~1.0",
		"ramsey/uuid": "~3.3",
		"guzzlehttp/guzzle": "~6.0",
		"nothingworks/blade-svg": "~0.1",

		"spatie/laravel-partialcache": "~1.2",
		"spatie/laravel-medialibrary": "~4.0",
		"spatie/laravel-menu": "~2.0",
		"spatie/laravel-backup": "~4.0",
		"spatie/laravel-activitylog": "~1.2"
	},

	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~5.0",
		"barryvdh/laravel-debugbar": "~2.1"
	},

	"autoload": {
		"classmap": [
			"nova/src/Setup/database"
		],
		"files": [
			"nova/src/Foundation/helpers.php"
		],
		"psr-4": {
			"Extensions\\": "extensions/",
			"Nova\\": "nova/src/"
		}
	},

	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},

	"config": {
		"vendor-dir": "nova/vendor",
		"bin-dir": "nova/vendor/bin",
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}
