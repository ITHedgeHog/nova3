{
	"name": "anodyne/nova3",
	"version": "3.0.0-alpha.2",
	"description": "Nova 3 RPG Management",
	"homepage": "http://anodyne-productions.com",
	"keywords": ["cms", "anodyne", "nova", "nova3"],
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "David VanScott",
			"email": "davidv@anodyne-productions.com",
			"role": "Developer"
		}
	],

	"config": {
		"vendor-dir": "nova/vendor",
		"bin-dir": "nova/vendor/bin",
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"require": {
		"php": ">=5.4.0",
		"laravel/framework": "~4.2",
		"erusev/parsedown": "~1.0",
		"ikimea/browser": "~1.0",
		"baum/baum": "~1.0",
		"thomaswelton/laravel-gravatar": "dev-master",
		"intervention/image": "~1.0",
		"doctrine/dbal": "2.3.*",
		"cartalyst/sentinel": "*",
		"cartalyst/sentry": "~2.0",
		"dingo/api": "~0.5",
		"laravelbook/ardent": "~2.4"
	},

	"require-dev": {
		"raveren/kint": "dev-master",
		"fzaninotto/faker": "*"
	},

	"repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],

	"autoload": {
		"classmap": [
			"nova/src/Setup/database/migrations",
			"nova/src/Setup/database/seeds"
		],

		"psr-4": {
			"App\\": "app/src/App/",
			"Modules\\": "app/src/Modules/",
			"Nova\\": "nova/src/"
		}
	},
	
	"minimum-stability": "dev",
	"prefer-stable": true
}