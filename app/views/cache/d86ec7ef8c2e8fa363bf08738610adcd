<div class="illuminate-query-pane">
	<?php if(isset($events['illuminate.query'])): ?>
		<div class="illuminate-table">
			<table width="100%">
				<tr>
					<th>Query</th>
					<th>Execution Time</th>
				</tr>
				<?php foreach($events['illuminate.query'] as $event): ?>
				<tr>
					<td><?php echo  $event['processed'] ; ?></td>
					<td><?php echo  number_format($event['time'], 3) . 'ms' ; ?></td>
				</tr>										
				<?php endforeach; ?>
			</table>
		</div>
	<?php else: ?>
		<p class="illuminate-empty">There have been no SQL queries executed.</p>
	<?php endif; ?>
</div>