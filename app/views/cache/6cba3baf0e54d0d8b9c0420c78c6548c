<div class="illuminate-query-pane">
	<?php if(count($profiler_routes)): ?>
		<div class="illuminate-table">
			<table width="100%">
				<tr>
					<th>Pattern</th>
					<th>Method</th>
					<th>Name</th>
				</tr>
				<?php foreach($profiler_routes as $name => $route): ?>
				<tr>
					<td><?php echo  $route->getPattern() ; ?></td>
					<td><?php $req = $route->getRequirements(); ?><?php echo  $req['_method'] ; ?></td>
					<td><?php if($name === md5($req['_method'].$route->getPattern())) $name = 'N/A'; ?><?php echo  $name ; ?></td>
				</tr>										
				<?php endforeach; ?>
			</table>
		</div>
	<?php else: ?>
		<p class="illuminate-empty">There are no active routes.</p>
	<?php endif; ?>
</div>